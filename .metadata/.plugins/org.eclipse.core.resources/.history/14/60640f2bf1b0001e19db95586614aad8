	<!-- form1-param 이라는 주소로 p1, p2에 입력된 값을 post 방식으로 전송 -->
	<form action="/form1-param" method="post">
	<!-- input 태그의 name 부분은 파라미터의 이름 -->
		p1: <input type="text" name="p1"> <br>
		p2: <input type="text" name="p2"> <br>
		<input type="submit" value="전송">
	</form>
</body>
</html>